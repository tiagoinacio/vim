let SessionLoad = 1
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/Work/betfair/bf-edw-static-client-nj/flavour/app/modules/betslip/source/scripts/controllers
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +242 ~/Work/betfair/bf-edw-static-client-nj/flavour/app/modules/betslip/source/scripts/controllers/receipt-controller.js
badd +159 ~/Work/betfair/bf-edw-static-client-nj/flavour/app/modules/betslip/source/scripts/controllers/betslip-controller.js
argglobal
silent! argdel *
argadd receipt-controller.js
edit ~/Work/betfair/bf-edw-static-client-nj/flavour/app/modules/betslip/source/scripts/controllers/receipt-controller.js
set splitbelow splitright
set nosplitbelow
wincmd t
set winheight=1 winwidth=1
argglobal
let s:l = 241 - ((24 * winheight(0) + 18) / 36)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
241
normal! 017|
tabnext 1
if exists('s:wipebuf') && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=84 shortmess=filnxtToOc
let s:sx = expand("<sfile>:p:r")."x.vim"
if file_readable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &so = s:so_save | let &siso = s:siso_save
doautoall SessionLoadPost
unlet SessionLoad
